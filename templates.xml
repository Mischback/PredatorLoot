<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ D:\UI.xsd">

	<!-- This is the template for a roll button -->
	<Button name="PredatorLootRollButtonTemplate" virtual="true">
		<Size><AbsDimension x="32" y="32"></AbsDimension></Size>	
		<Scripts>
			<OnLoad>self:RegisterForClicks('LeftButtonUp')</OnLoad>
			<OnLeave>GameTooltip:Hide()</OnLeave>
		</Scripts>
	</Button>

	<!--  -->
	<Button name="PredatorLootRollStartButtonTemplate" inherits="PredatorLootRollButtonTemplate" virtual="true">
		<NormalTexture file="Interface\Addons\PredatorLoot\gfx\icon_start" />
		<HighlightTexture file="Interface\Addons\PredatorLoot\gfx\icon_start_highlight" />
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, 'ANCHOR_TOPLEFT')
				GameTooltip:SetText(PredatorLootStrings.MasterLooterStartButton)
			</OnEnter>
		</Scripts>
	</Button>

	<!--  -->
	<Button name="PredatorLootRollEndButtonTemplate" inherits="PredatorLootRollButtonTemplate" virtual="true">
		<NormalTexture file="Interface\Addons\PredatorLoot\gfx\icon_stop" />
		<HighlightTexture file="Interface\Addons\PredatorLoot\gfx\icon_stop_highlight" />
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, 'ANCHOR_TOPLEFT')
				GameTooltip:SetText(PredatorLootStrings.MasterLooterEndButton)
			</OnEnter>
		</Scripts>
	</Button>

	<!--  -->
	<Button name="PredatorLootAnnounceRulesButtonTemplate" inherits="PredatorLootRollButtonTemplate" virtual="true">
		<NormalTexture file="Interface\Addons\PredatorLoot\gfx\icon_announce" />
		<HighlightTexture file="Interface\Addons\PredatorLoot\gfx\icon_announce_highlight" />
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, 'ANCHOR_TOPLEFT')
				GameTooltip:SetText(PredatorLootStrings.MasterLooterAnnounceRulesButton)
			</OnEnter>
			<OnClick>PredatorLootFunctions.AnnounceLootRules()</OnClick>
		</Scripts>
	</Button>

	<!--  -->
	<Button name="PredatorLootConfigButtonTemplate" inherits="PredatorLootRollButtonTemplate" virtual="true">
		<NormalTexture file="Interface\Addons\PredatorLoot\gfx\icon_config" />
		<HighlightTexture file="Interface\Addons\PredatorLoot\gfx\icon_config_highlight" />
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, 'ANCHOR_TOPLEFT')
				GameTooltip:SetText(PredatorLootStrings.MasterLooterConfigButton)
			</OnEnter>
		</Scripts>
	</Button>

	<!-- Template for "Need"-button (dice) -->
	<Button name="PredatorLootRollButtonNeedTemplate" inherits="PredatorLootRollButtonTemplate" virtual="true">
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, 'ANCHOR_TOPLEFT')
				GameTooltip:SetText(NEED)
			</OnEnter>
		</Scripts>
		<NormalTexture file="Interface\Buttons\UI-GroupLoot-Dice-Up" />
		<HighlightTexture file="Interface\Buttons\UI-GroupLoot-Dice-Highlight" alphaMode="ADD" />
		<PushedTexture file="Interface\Buttons\UI-GroupLoot-Dice-Down" />
	</Button>
	
	<!-- Template for "Greed"-button (coin) -->
	<Button name="PredatorLootRollButtonGreedTemplate" inherits="PredatorLootRollButtonTemplate" virtual="true">
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, 'ANCHOR_TOPLEFT')
				GameTooltip:SetText(GREED)
			</OnEnter>
		</Scripts>
		<NormalTexture file="Interface\Buttons\UI-GroupLoot-Coin-Up" />
		<HighlightTexture file="Interface\Buttons\UI-GroupLoot-Coin-Highlight" alphaMode="ADD" />
		<PushedTexture file="Interface\Buttons\UI-GroupLoot-Coin-Down" />
	</Button>

	<!-- Template for "DisEnchant"-button -->
	<Button name="PredatorLootRollButtonDisEnchantTemplate" inherits="PredatorLootRollButtonTemplate" virtual="true">
		<Scripts>
			<OnEnter>
				GameTooltip:SetOwner(self, 'ANCHOR_TOPLEFT')
