	<!-- This is the string which is used for the item name -->
	<FontString name="PredatorLootFrameNameTemplate" font="Fonts\FRIZQT__.TTF" text="ItemName" justifyH="LEFT" virtual="true">
		<FontHeight>
			<AbsValue val="12"/>
		</FontHeight>
		<Shadow>
			<Offset>
				<AbsDimension x="1" y="1"/>
			</Offset>
			<Color r="0" g="0" b="0"/>
		</Shadow>
	</FontString>

	<!-- This is the string which is used for the item count -->
	<FontString name="PredatorLootFrameCountTemplate" font="Fonts\FRIZQT__.TTF" text="" virtual="true">
		<FontHeight>
			<AbsValue val="11"/>
		</FontHeight>
		<Shadow>
			<Offset>
				<AbsDimension x="1" y="1"/>
			</Offset>
			<Color r="0" g="0" b="0"/>
		</Shadow>
	</FontString>

