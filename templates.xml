					<Anchors>
						<Anchor point="BOTTOMLEFT">
							<Offset><AbsDimension x="-4" y="-4"/></Offset>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="0.33333" top="0.66666" bottom="1" />
				</Texture>
				<Texture name="$parentBorderBottomRight" file="Interface\AddOns\PredatorLoot\gfx\border">
					<Size><AbsDimension x="10" y="10" /></Size>
					<Anchors>
						<Anchor point="BOTTOMRIGHT">
							<Offset><AbsDimension x="4" y="-4"/></Offset>
						</Anchor>
					</Anchors>
					<TexCoords left="0.66666" right="1" top="0.66666" bottom="1" />
