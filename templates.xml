							<Offset><AbsDimension x="-3" y="2" /></Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
			<Layer level="BORDER">
				<Texture name="$parentBorderTopLeft" file="Interface\AddOns\PredatorLoot\gfx\border">
